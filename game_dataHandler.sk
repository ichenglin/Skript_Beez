function game_dataHandler_invisiblePressurePlate_register(location: location, action: text):
	set {_locationBlock} to (block at {_location})
	set {_locationText} to "%{_locationBlock}'s x coords%_%{_locationBlock}'s y coords%_%{_locationBlock}'s z coords%"
	set {-game_dataHandler::PRESSURE_PLATE_LIST} to (check [{-game_dataHandler::PRESSURE_PLATE_LIST} is set] ? "%{-game_dataHandler::PRESSURE_PLATE_LIST}%:%{_locationText}%" : "%{_locationText}%")
	set {-game_dataHandler::%{_locationText}%_PRESSURE_PLATE_LOCATION} to {_location}
	set {-game_dataHandler::%{_locationText}%_PRESSURE_PLATE_ACTION} to {_action}
	set block at {_location} to STONE_PRESSURE_PLATE