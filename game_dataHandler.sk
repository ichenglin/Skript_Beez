function game_dataHandler_public_linkItemToID(item: item, itemID: text) :: item:
	set {_item} to {_item} with lore "%{_item}'s lore%||&f||&7&oID: %{_itemID} in lower case%"
	return {_item}

function game_dataHandler_public_textToItems(text: text, receiver: player) :: items:
	# ITEM:1~2 (1 to 2 items),ITEM:1 (garentee amount),ITEM:1:[10] (10% chance to get 1)
	# BOOSTER:Ore:2:60 (Booster for "X2 Ore" and last for 60 seconds)
	set {_itemsSplit::*} to {_text} split by ","
	loop {_itemsSplit::*}:
		set {_loopItemSplit::*} to {_itemsSplit::%loop-index%} split by ":"
		if {_loopItemSplit::1} is "BOOSTER":
			game_boosterHandler_new({_receiver}, "%{_loopItemSplit::2}%:%{_loopItemSplit::3}%", ("%{_loopItemSplit::4}% seconds" parsed as time span))
		else if {-game_dataHandler::%{_loopItemSplit::1}%} is set:
			if {_loopItemSplit::2} contains "~":
				set {_amountRangeSplit::*} to {_loopItemSplit::2} split by "~"
				set {_low} to ({_amountRangeSplit::1} parsed as number)
				set {_high} to ({_amountRangeSplit::2} parsed as number)
				set {_amount} to (random integer between {_low} and {_high})
			else:
				set {_amount} to ({_loopItemSplit::2} parsed as number)
			if {_loopItemSplit::3} is set:
				set {_chance} to ({_loopItemSplit::3} parsed as number)
				set {_random} to (random number between 0 and 100)
				if {_chance} >= {_random}:
					set {_chanceItem} to ({_amount} of {-game_dataHandler::%{_loopItemSplit::1}%})
					if {_chance} <= 5:
						if {_receiver} is online:
							send "&d%{_receiver}% has found %game_dataHandler_public_getItemDisplay({_chanceItem})%! (%{_chance}%%%)" to all players
					add {_chanceItem} to {_returnItems::*}
					game_questHandler_recordStat({_receiver}, "COLLECTING_%{_loopItemSplit::1}%", {_amount})
			else:
				add ({_amount} of {-game_dataHandler::%{_loopItemSplit::1}%}) to {_returnItems::*}
				game_questHandler_recordStat({_receiver}, "COLLECTING_%{_loopItemSplit::1}%", {_amount})
	return {_returnItems::*}

function game_dataHandler_public_getOreTypes() :: items:
	set {_oreTypesRaw::*} to {-game_dataHandler::ALL_ORES} split by ":"
	loop {_oreTypesRaw::*}:
		set {_oreTypesRaw::%loop-index%} to ({_oreTypesRaw::%loop-index%} parsed as item)
	return {_oreTypesRaw::*}

function game_dataHandler_public_getItemDisplay(item: item) :: text:
	return "%uncolored {_item}'s name% x%{_item}'s item amount%"

function game_dataHandler_public_checkInArea(location: location, restrictTypes: texts) :: boolean:
	loop {_restrictTypes::*}:
		set {_loopRestrictType} to {_restrictTypes::%loop-index%}
		if {_loopRestrictType} is "ORE_MINES":
			set {_oreTypes::*} to game_dataHandler_public_getOreTypes()
			loop {_oreTypes::*}:
				set {_loopOreType} to {_oreTypes::%loop-index-2%}
				add game_dataHandler_oreArea_getPos({_loopOreType}) to {_regionPoints::*}
		else if {_loopRestrictType} is "SPAWN":
			add game_dataHandler_area_getPos("SPAWN") to {_regionPoints::*}
	loop ((amount of {_regionPoints::*}) / 2) times:
		set {_pos1Num} to ((loop-number * 2) - 1)
		set {_pos2Num} to (loop-number * 2)
		if game_dataHandler_public_checkInArea_checkBetweenPoints({_regionPoints::%{_pos1Num}%}, {_regionPoints::%{_pos2Num}%}, {_location}) is true:
			return true
	return false

function game_dataHandler_public_checkInArea_checkBetweenPoints(regionLocation1: location, regionLocation2: location, checkLocation: location) :: boolean:
	add (({_regionLocation1}'s x coords) and ({_regionLocation2}'s x coords) and ({_checkLocation}'s x coords) and ({_regionLocation1}'s y coords) and ({_regionLocation2}'s y coords) and ({_checkLocation}'s y coords) and ({_regionLocation1}'s z coords) and ({_regionLocation2}'s z coords) and ({_checkLocation}'s z coords)) to {_coords::*}
	loop 3 times:
		delete {_loopNums::*}
		add (loop-number * 3 - 2) and (loop-number * 3 - 1) and (loop-number * 3) to {_loopNums::*}
		if {_coords::%{_loopNums::1}%} > {_coords::%{_loopNums::2}%}:
			set {_high} to {_coords::%{_loopNums::1}%}
			set {_low} to {_coords::%{_loopNums::2}%}
		else:
			set {_high} to {_coords::%{_loopNums::2}%}
			set {_low} to {_coords::%{_loopNums::1}%}
		if {_coords::%{_loopNums::3}%} > {_high}:
			return false
		else if {_coords::%{_loopNums::3}%} < {_low}:
			return false
	return true

function game_dataHandler_items_register(itemID: text, item: item):
	set {-game_dataHandler::%{_itemID}%_ITEM} to game_dataHandler_public_linkItemToID({_item}, "%{_itemID}%_ITEM")

function game_dataHandler_items_getItem(itemID: text, amount: number = 1) :: item:
	return ({_amount} of {-game_dataHandler::%{_itemID}%_ITEM})

function game_dataHandler_oreBlocks_register(oreBlock: item, oreDropItemID: text, oreDropAmount: number = 1):
	set {_oreID} to game_dataHandler_oreBlocks_nameToID({_oreBlock})
	set {-game_dataHandler::%{_oreID}%_DROPS} to game_dataHandler_items_getItem({_oreDropItemID}, {_oreDropAmount})

function game_dataHandler_oreBlocks_checkValid(oreBlock: item) :: boolean:
	set {_oreID} to game_dataHandler_oreBlocks_nameToID({_oreBlock})
	return (check [{-game_dataHandler::%{_oreID}%_DROPS} is set])

function game_dataHandler_oreBlocks_getData(oreBlock: item) :: objects:
	set {_oreID} to game_dataHandler_oreBlocks_nameToID({_oreBlock})
	add {-game_dataHandler::%{_oreID}%_DROPS} to {_data::*}
	return {_data::*}

function game_dataHandler_oreBlocks_nameToID(oreBlock: item) :: text:
	set {_oreID} to "%{_oreBlock}%" in upper case
	replace all " " with "_" in {_oreID}
	return {_oreID}

function game_dataHandler_oreArea_register(oreBlock: item, location1: location, location2: location):
	set {_oreID} to game_dataHandler_oreBlocks_nameToID({_oreBlock})
	set {-game_dataHandler::%{_oreID}%_ORE_AREA_POS_ONE} to {_location1}
	set {-game_dataHandler::%{_oreID}%_ORE_AREA_POS_TWO} to {_location2}

function game_dataHandler_oreArea_getPos(oreBlock: item) :: locations:
	set {_oreID} to game_dataHandler_oreBlocks_nameToID({_oreBlock})
	add {-game_dataHandler::%{_oreID}%_ORE_AREA_POS_ONE} to {_returnPos::*}
	add {-game_dataHandler::%{_oreID}%_ORE_AREA_POS_TWO} to {_returnPos::*}
	return {_returnPos::*}

function game_dataHandler_area_register(name: text, location1: location, location2: location):
	set {-game_dataHandler::%{_name}%_AREA_POS_ONE} to {_location1}
	set {-game_dataHandler::%{_name}%_AREA_POS_TWO} to {_location2}

function game_dataHandler_area_getPos(name: text) :: locations:
	add {-game_dataHandler::%{_name}%_AREA_POS_ONE} to {_returnPos::*}
	add {-game_dataHandler::%{_name}%_AREA_POS_TWO} to {_returnPos::*}
	return {_returnPos::*}

