function game_dataHandler_mob_registerMob(type: text, name: text, damage: number, reach: number, maxHealth: number, protection: number, speed: number, loot: text, location: location):
	spawn ({_type} parsed as entity type) at {_location}
	set {_mob} to last spawned entity
	set {_mobUUID} to ({_mob}'s UUID)
	set {-game_dataHandler::MOB_LIST} to (check [{-game_dataHandler::MOB_LIST} is set] ? "%{-game_dataHandler::MOB_LIST}%:%{_mobUUID}%" : "%{_mobUUID}%")
	set {-entityData::%{_mobUUID}%_ENTITY} to {_mob}
	set {-entityData::%{_mobUUID}%_NAME} to {_name}
	set {-entityData::%{_mobUUID}%_DAMAGE} to {_damage}
	set {-entityData::%{_mobUUID}%_REACH} to {_reach}
	set {-entityData::%{_mobUUID}%_HEALTH_MAX} to {_maxHealth}
	set {-entityData::%{_mobUUID}%_PROTECTION} to {_protection}
	set {-entityData::%{_mobUUID}%_SPEED} to {_speed}
	set {-entityData::%{_mobUUID}%_LOOT_ITEMS} to {_loot}
	set {-entityData::%{_mobUUID}%_SPAWN_LOCATION} to {_location}
	set {-entityData::%{_mobUUID}%_DEATH_COUNT} to 0
	set the name of {_mob} to {_name}
#	set custom name visibility of {_mob} to true
	game_healthbarHandler_new({_mob})
	game_healthbarHandler_update({_mob})
	add "{NoAI:1}" to entity-nbt of {_mob}

