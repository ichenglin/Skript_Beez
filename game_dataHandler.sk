on script unload:
	loop all players:
		game_playerInventoryHandler_saveInventory(loop-player)
	game_mobHandler_killAll()

on script load:
	wait 1 tick
	kill all animals
	kill all monsters
	kill all area effect clouds
	wait 1 tick
	game_dataHandler_update()
	wait 1 tick
	loop all players:
		game_playerInventoryHandler_loadInventory(loop-player)
		game_healthbarHandler_new(loop-player)
		game_healthbarHandler_update(loop-player)

